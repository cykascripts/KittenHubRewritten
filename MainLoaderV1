-- Scripts:

local function TVWOXR_fake_script() -- KHRText.LocalScript 
	local script = Instance.new('LocalScript', KHRText)

	script.Parent.Parent.InnerCircle.Rotation = 10
	wait(0.1)
	script.Parent.Parent.InnerCircle.Rotation = 20
	wait(0.1)
	script.Parent.Parent.InnerCircle.Rotation = 30
	wait(0.1)
	script.Parent.Parent.InnerCircle.Rotation = 40
	wait(0.1)
	script.Parent.Parent.InnerCircle.Rotation = 50
	wait(0.1)
	script.Parent.Parent.InnerCircle.Rotation = 60
	wait(0.1)
	script.Parent.Parent.InnerCircle.Rotation = 70
	wait(0.1)
	script.Parent.Parent.InnerCircle.Rotation = 80
	wait(0.1)
	script.Parent.Parent.InnerCircle.Rotation = 90
	wait(0.1)
	script.Parent.Parent.InnerCircle.Rotation = 100
	wait(1)	
	wait(0.5)
	if game.PlaceId == 2512643572 then
	loadstring(game:HttpGet('https://raw.githubusercontent.com/cykascripts/KittenHubRewritten/master/BubblegumSim'))()
	else
	if game.PlaceId == 189707 then
	loadstring(game:HttpGet('https://raw.githubusercontent.com/cykascripts/KittenHubRewritten/master/NaturalDisasterSurvival'))()
	end
	if game.PlaceId == 537413528 then
	loadstring(game:HttpGet('https://raw.githubusercontent.com/cykascripts/KittenHubRewritten/master/BuildABoat'))()
	end
	if game.PlaceId == 155615604 then
	loadstring(game:HttpGet('https://raw.githubusercontent.com/cykascripts/KittenHubRewritten/master/PrisonLife'))()
	end
	if game.PlaceId == 1962086868 then
	loadstring(game:HttpGet('https://raw.githubusercontent.com/cykascripts/KittenHubRewritten/master/TowerOfHell'))()
	end
    script.Parent.Parent:Destroy()
    end

end
coroutine.wrap(TVWOXR_fake_script)()
